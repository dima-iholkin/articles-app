{
  "AllowedHosts": "*",
  "Custom": {
    "ElasticSearch": {
      "AutoRegisterTemplate": true,
      "AutoRegisterTemplateVersion": "ESv7"
    },
    "IdentityServer": {
      "IssuerUri": "ArticlesApp",
      "SPA": {
        "ClientId": "ArticlesApp.WebAPI",
        "RedirectUris": [
          "/authentication/login-callback",
          "/silent-refresh"
        ],
        "PostLogoutRedirectUris": [
          "/authentication/logout-callback"
        ],
        "AdditionalRoles": [
          "roles"
        ]
      }
    }
  }
}